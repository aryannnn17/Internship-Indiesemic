
&uart1 {
    status = "disabled";
};

&uart1 {
    status = "okay";
};

&uart1_default {
    group1 {
        psels = <NRF_PSEL(UART_TX, 1, 11)>;
    };
};

&uart1_default {
    group2 {
        psels = <NRF_PSEL(UART_RX, 1, 12)>;
    };
};

&uart1_sleep {
    group1 {
        psels = <NRF_PSEL(UART_TX, 1, 11)>, <NRF_PSEL(UART_RX, 1, 12)>;
    };
};

&uart1_sleep {
    group1 {
        psels = <NRF_PSEL(UART_TX, 1, 11)>, <NRF_PSEL(UART_RX, 1, 12)>;
    };
};
